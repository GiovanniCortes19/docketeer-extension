{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2021",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "skipLibCheck": true,
    //TODO: Changed to false
    "noImplicitAny": false,
    "strictNullChecks": false,
    "baseUrl": "./",
    "allowSyntheticDefaultImports": true,
    "paths": { "@docker/*": ["./node_modules/@docker"] }
  },
  "exclude": ["./__mocks__/", "./__tests__"],
  "include": ["./tests/setup.ts"]
}
