{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2021",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "skipLibCheck": true,
